# Frontend Production Environment Variables
# Domain: www.ilai.co.in

# API Base URL
VITE_API_BASE_URL=https://www.ilai.co.in/api

# Authentication Mode: 'keycloak' or 'local'
VITE_AUTH_MODE=keycloak

# Keycloak Configuration (Path-based routing)
VITE_KEYCLOAK_URL=https://www.ilai.co.in
VITE_KEYCLOAK_REALM=ilai
VITE_KEYCLOAK_CLIENT_ID=ilai-frontend

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_ERROR_REPORTING=true
