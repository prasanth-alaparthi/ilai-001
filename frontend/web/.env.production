# Frontend Production Environment Variables
# Domain: ilai.co.in

# API Base URL (empty for relative paths - nginx proxies to services)
VITE_API_BASE_URL=

# Authentication Mode: 'keycloak' or 'local'
VITE_AUTH_MODE=local

# Keycloak Configuration (commented out for local auth)
# VITE_KEYCLOAK_URL=https://ilai.co.in
# VITE_KEYCLOAK_REALM=ilai
# VITE_KEYCLOAK_CLIENT_ID=ilai-frontend

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_ERROR_REPORTING=true
